<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('<%= banner_restaurant_photo(@restaurant) %>');">
</div>
<div class="banner-logo-avatar" style="background-image: url('<%= logo_restaurant_photo(@restaurant) %>');">
</div>

<h1><%= @restaurant.name %></h1>

<ul class="list-inline">
  <li>All</li>
  <% Dish::CATEGORY.each do |category| %>
    <li><%= category %></li>
  <% end %>
</ul>
